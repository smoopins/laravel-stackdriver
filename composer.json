{
    "name": "smoopins/laravel-stackdriver",
    "description": "Enables logging, tracing and error reporting to Google Stackdriver for the Laravel framework, forked from GlueDev/laravel-stackdriver",
    "license": "MIT",
    "homepage": "https://github.com/smoopins/laravel-stackdriver",
    "keywords": [
        "Laravel",
        "Google",
        "Stackdriver",
        "Tracing",
        "Logging",
        "Error Reporting"
    ],
    "require": {
        "illuminate/support": "^9.5",
        "google/cloud-error-reporting": "^0.16",
        "google/cloud-logging": "^1.23",
        "opencensus/opencensus": "^0.7.0",
        "opencensus/opencensus-exporter-stackdriver": "^0.1",
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "~7.2",
        "sempro/phpunit-pretty-print": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Smoopins\\Laravel\\Stackdriver\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Smoopins\\Laravel\\Stackdriver\\StackdriverServiceProvider"
            ]
        }
    },
    "archive": {
        "exclude": [
            "storage/screenshots"
        ]
    }
}
